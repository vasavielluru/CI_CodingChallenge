pipeline
{
 agent {
    docker {
      image 'maven:3.6.3-jdk-11'
      args '-v /root/.m2:/root/.m2'
    }
 }
   stages{
    stage('Build') {
      steps {
        sh 'mvn -f PipelineCC/pom.xml compile'
        echo 'Building'
      }
   }
   stage('Test') {
      steps  {
          sh 'mvn -f PipelineCC/pom.xml  test'
          echo 'Testing'
       }
    }
    
    
  }
}
