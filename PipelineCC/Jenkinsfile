pipeline
{
  agent any
  tools
  {
    maven 'Maven'
    jdk 'jdk8'
   }
   stages{
    stage('Build') {
      steps {
        sh 'mvn -f PipelineCC/pom.xml compile'
        echo 'Building'
      }
   }
   stage('Test') {
      steps  {
          sh 'mvn  test'
          echo 'Testing'
       }
    }
  }
}
